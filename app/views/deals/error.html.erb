<%= render 'layouts/biz/dash' %>
<div class="contentWrapper">
  <div class="content">

    <!--Flash messages-->
    <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>

    <!-- Content Heading -->
    <div class="pull-left">
      <p class="b2 content-title">Deal Listing</p>
    </div>

    <div class="clearfix"></div>
    <div class="content-separator"></div>

    <!-- Content Body -->
    <div class = "well col-lg-6 col-lg-offset-3" style="text-align: center; font-weight: bold">
      <p>Sorry, you have not subscribe to our Deal Listing Service </p>
      <%= link_to 'Subscribe now', new_payment_path, class: "btn btn-lg btn-info" %>
    </div>

  </div><!-- /content -->
</div><!-- /contentWrapper -->

<%= content_for :lean_overlay do %>
<% end %>

<%= content_for :body_footer do %>
<% end %>


